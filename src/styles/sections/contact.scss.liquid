@charset "UTF-8";


#contact-muck {
    float: right;
    width: 70%;
    margin-right: 10%;
    margin-top: 50px;
    background-color: #164732;
}

#contact-muck form {
    width: 60%;
    margin: 0 auto;
    padding: 50px 0;
}

#contact-muck form label {
    color: #fff;
}

#contact-muck form input[type="submit"] {
    background-color: #ab5232;
}

#contact-muck input::placeholder {
    color: transparent;
    opacity: 0; /* Firefox */
}

#contact-muck input:-ms-input-placeholder { /* Internet Explorer 10-11 */
   color: transparent;
}

#contact-muck input::-ms-input-placeholder { /* Microsoft Edge */
   color: transparent;
}

#contact-muck textarea::-webkit-input-placeholder { /* Chrome/Opera/Safari */
  color: transparent;
}
#contact-muck textarea::-moz-placeholder { /* Firefox 19+ */
  color: transparent;
}
#contact-muck textarea:-ms-input-placeholder { /* IE 10+ */
  color: transparent;
}
#contact-muck textarea:-moz-placeholder { /* Firefox 18- */
  color: transparent;
}